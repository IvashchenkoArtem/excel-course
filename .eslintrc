{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
      "semi": "off",
      "indent": "off",
      "arrow-parens": "off",
      "comma-dangle": "off",
      "quotes": "off",
      "require-jsdoc": "off",
      "eol-last": "off",
      "operator-linebreak": "off"
  },
  "extends": ["eslint:recommended", "google"]
}